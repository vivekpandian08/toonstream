          "require_lowercase": true,
          "require_numbers": true,
          "require_special": true,
          "bcrypt_cost": 12
        },
        "oauth": {
          "google": {
            "enabled": true,
            "client_id": "google-client-id",
            "client_secret_secret": "google-client-secret",
