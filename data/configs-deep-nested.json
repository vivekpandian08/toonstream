    "environment": "production",
    "deployment": {
      "strategy": "rolling",
      "replicas": 15,
      "resources": {
        "cpu": {
          "request": "2000m",
          "limit": "8000m"
        },
        "memory": {
          "request": "4Gi",
          "limit": "16Gi"
        }
      }
    },
    "cdn": {
      "providers": {
        "primary": {
          "name": "cloudflare",
          "zones": ["zone1", "zone2", "zone3"],
          "cache_levels": {
            "static": {
              "ttl": "30d",
