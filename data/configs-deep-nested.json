  {
    "service_name": "inventory-service",
    "version": "1.9.1",
    "environment": "production",
    "deployment": {
      "strategy": "canary",
      "replicas": 5,
      "resources": {
        "cpu": {
          "request": "1500m",
          "limit": "6000m"
        },
        "memory": {
          "request": "3Gi",
          "limit": "12Gi"
        }
      }
    },
    "database": {
      "mongodb": {
        "connection_string_secret": "mongodb-connection",
        "database": "inventory",
        "collections": {
          "products": {
            "indexes": [
              {"keys": {"sku": 1}, "unique": true},
              {"keys": {"category": 1, "price": -1}},
              {"keys": {"name": "text"}}
            ]
          },
          "stock": {
            "time_series": {
              "enabled": true,
              "time_field": "timestamp",
              "meta_field": "product_id"
            }
          }
        },
        "read_preference": "secondaryPreferred",
        "write_concern": {
          "w": "majority",
          "j": true
        }
      }
    },
    "sync": {
      "warehouses": {
        "main": {
          "api_endpoint": "https://warehouse-main.internal",
          "sync_interval": "5m",
          "batch_size": 100
        },
        "backup": {
          "api_endpoint": "https://warehouse-backup.internal",
          "sync_interval": "15m",
          "batch_size": 50
        }
      },
      "conflict_resolution": {
        "strategy": "last-write-wins",
        "threshold": "5m"
      }
    }
  },
  {
    "service_name": "recommendation-engine",
    "version": "2.7.3",
    "environment": "production",
    "deployment": {
      "strategy": "rolling",
      "replicas": 12,
      "resources": {
        "cpu": {
          "request": "4000m",
          "limit": "16000m"
        },
        "memory": {
          "request": "16Gi",
          "limit": "64Gi"
        }
      }
    },
    "models": {
      "collaborative_filtering": {
        "algorithm": "als",
        "hyperparameters": {
          "rank": 50,
          "max_iter": 10,
          "reg_param": 0.01,
          "alpha": 1.0
        },
        "training": {
          "schedule": "0 2 * * *",
          "min_interactions": 5,
          "validation_split": 0.2
        }
      },
      "content_based": {
        "algorithm": "cosine_similarity",
        "features": {
          "text": {
            "vectorizer": "tfidf",
            "max_features": 5000,
            "ngram_range": [1, 2]
          },
          "categorical": {
            "encoding": "one_hot"
          }
        }
      },
      "hybrid": {
        "enabled": true,
        "weights": {
          "collaborative": 0.6,
          "content": 0.3,
          "popularity": 0.1
        }
      }
    },
    "serving": {
      "cache": {
        "redis": {
          "ttl": "1h",
          "max_recommendations": 50
        }
      },
      "fallback": {
        "enabled": true,
        "strategy": "trending"
      },
      "ab_testing": {
        "enabled": true,
        "variants": {
          "control": {
            "percentage": 50,
            "model": "collaborative"
          },
          "experiment": {
            "percentage": 50,
            "model": "hybrid"
          }
        }
      }
    }
  },
  {
    "service_name": "fraud-detection",
    "version": "1.4.8",
    "environment": "production",
    "deployment": {
      "strategy": "blue-green",
      "replicas": 8,
      "resources": {
        "cpu": {
          "request": "3000m",
          "limit": "12000m"
        },
        "memory": {
          "request": "8Gi",
          "limit": "32Gi"
        }
      }
    },
    "rules_engine": {
      "rules": {
        "velocity": {
          "enabled": true,
          "thresholds": {
            "transactions_per_hour": 10,
            "amount_per_day": 5000
          },
          "action": "flag"
        },
        "geolocation": {
          "enabled": true,
          "impossible_travel": {
            "max_speed_kmh": 800,
            "time_window": "4h"
          },
          "action": "block"
        },
        "device": {
          "fingerprinting": {
            "enabled": true,
            "max_devices_per_user": 5
          },
          "new_device": {
            "require_verification": true,
            "cooldown": "24h"
          }
        }
      }
    },
    "ml_models": {
      "anomaly_detection": {
        "algorithm": "isolation_forest",
        "parameters": {
          "n_estimators": 100,
          "contamination": 0.01
        },
        "features": ["amount", "merchant_category", "time_of_day", "day_of_week"],
        "threshold": 0.95
      },
      "risk_scoring": {
        "model_type": "xgboost",
        "version": "v3.2",
        "endpoint": "https://ml.internal/fraud-score",
        "timeout": "100ms",
        "fallback_score": 0.5
      }
    },
    "actions": {
      "flag": {
        "notify": ["fraud-team@example.com"],
        "require_manual_review": false
      },
      "block": {
        "notify": ["fraud-team@example.com", "security@example.com"],
        "user_notification": true
      },
      "challenge": {
        "methods": ["sms", "email", "totp"],
        "timeout": "5m"
      }
    }
  },
