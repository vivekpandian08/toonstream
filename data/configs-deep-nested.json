[
  {
    "service_name": "api-gateway",
    "version": "2.4.1",
    "environment": "production",
    "deployment": {
      "strategy": "rolling",
      "replicas": 3,
      "resources": {
        "cpu": {
          "request": "500m",
          "limit": "2000m"
        },
        "memory": {
          "request": "1Gi",
          "limit": "4Gi"
        }
      },
      "health_check": {
        "liveness": {
          "http_get": {
            "path": "/health",
            "port": 8080,
            "scheme": "HTTP"
          },
          "initial_delay": 30,
          "period": 10
        },
        "readiness": {
          "http_get": {
            "path": "/ready",
            "port": 8080,
            "scheme": "HTTP"
          },
          "initial_delay": 10,
          "period": 5
        }
      }
    },
    "network": {
      "ingress": {
        "enabled": true,
        "host": "api.example.com",
        "tls": {
          "enabled": true,
          "certificate": "wildcard-cert",
          "min_version": "TLSv1.2"
        },
        "rate_limiting": {
          "enabled": true,
          "requests_per_second": 1000,
          "burst": 2000
        }
      },
      "egress": {
        "allowed_destinations": ["*.aws.amazon.com", "*.google.com"],
        "proxy": {
          "enabled": true,
          "host": "proxy.internal",
          "port": 3128
        }
      }
    },
    "observability": {
      "logging": {
        "level": "info",
        "format": "json",
        "outputs": ["stdout", "file"],
        "rotation": {
          "max_size": "100MB",
          "max_age": "7d",
          "max_backups": 10
        }
      },
      "metrics": {
        "enabled": true,
        "port": 9090,
        "path": "/metrics",
        "scrape_interval": "15s"
      },
      "tracing": {
        "enabled": true,
        "sampler": {
          "type": "probabilistic",
          "param": 0.1
        },
        "exporter": {
          "endpoint": "jaeger-collector:14250",
          "protocol": "grpc"
        }
      }
    }
  },
  {
    "service_name": "user-service",
    "version": "1.8.3",
    "environment": "production",
    "deployment": {
      "strategy": "blue-green",
      "replicas": 5,
      "resources": {
        "cpu": {
          "request": "1000m",
          "limit": "4000m"
        },
        "memory": {
          "request": "2Gi",
          "limit": "8Gi"
        }
      },
      "health_check": {
        "liveness": {
          "tcp_socket": {
            "port": 5432
          },
          "initial_delay": 60,
          "period": 20
        },
        "readiness": {
          "exec": {
            "command": ["/bin/check-db", "--timeout=5"]
          },
          "initial_delay": 15,
          "period": 10
        }
      }
    },
    "database": {
      "primary": {
        "host": "postgres-master.db.internal",
        "port": 5432,
        "name": "users",
        "ssl": {
          "enabled": true,
          "mode": "require",
          "ca_cert": "/certs/ca.pem"
        },
        "pool": {
          "min_size": 5,
          "max_size": 20,
          "max_idle_time": "10m"
        }
      },
      "replica": {
        "host": "postgres-replica.db.internal",
        "port": 5432,
        "lag_threshold": "1s",
        "failover": {
          "enabled": true,
          "timeout": "30s"
        }
      }
    },
    "cache": {
      "redis": {
        "cluster": {
          "nodes": ["redis-1:6379", "redis-2:6379", "redis-3:6379"],
          "mode": "cluster",
          "password_secret": "redis-password"
        },
        "settings": {
          "max_retries": 3,
          "timeout": "2s",
          "ttl": "1h"
        }
      }
    },
    "security": {
      "authentication": {
        "jwt": {
          "issuer": "auth.example.com",
          "audience": "user-service",
          "algorithm": "RS256",
          "public_key_url": "https://auth.example.com/.well-known/jwks.json"
        }
      },
      "authorization": {
        "rbac": {
          "enabled": true,
          "default_role": "user",
          "admin_roles": ["admin", "superadmin"]
        }
      }
    }
  },
  {
    "service_name": "payment-processor",
    "version": "3.1.0",
    "environment": "production",
    "deployment": {
      "strategy": "canary",
      "replicas": 4,
      "canary": {
        "steps": [10, 25, 50, 100],
        "interval": "5m",
        "metrics": {
          "success_rate": {
            "threshold": 0.99,
            "query": "sum(rate(http_requests_total{status=~\"2..\"}[5m]))"
          }
        }
      },
      "resources": {
        "cpu": {
          "request": "2000m",
          "limit": "8000m"
        },
        "memory": {
          "request": "4Gi",
          "limit": "16Gi"
        }
      }
    },
    "integrations": {
      "stripe": {
        "enabled": true,
        "api_version": "2023-10-16",
        "webhook": {
          "endpoint": "/webhooks/stripe",
          "signing_secret": "stripe-webhook-secret",
          "events": ["payment_intent.succeeded", "payment_intent.failed"]
        },
        "retry": {
          "max_attempts": 3,
          "backoff": {
            "initial": "1s",
            "multiplier": 2,
            "max": "30s"
          }
        }
      },
      "paypal": {
        "enabled": true,
        "mode": "live",
        "client_credentials": {
          "client_id_secret": "paypal-client-id",
          "client_secret_secret": "paypal-client-secret"
        }
      }
    },
    "message_queue": {
      "kafka": {
        "brokers": ["kafka-1:9092", "kafka-2:9092", "kafka-3:9092"],
        "topics": {
          "payment_events": {
            "partitions": 10,
            "replication_factor": 3,
            "retention": "7d"
          }
        },
        "producer": {
          "acks": "all",
          "compression": "snappy",
          "batch_size": 16384
        },
        "consumer": {
          "group_id": "payment-processor",
          "auto_offset_reset": "earliest",
          "enable_auto_commit": false
        }
      }
    }
  },
  {
    "service_name": "notification-engine",
    "version": "2.0.5",
    "environment": "production",
    "deployment": {
      "strategy": "rolling",
      "replicas": 6,
      "resources": {
        "cpu": {
          "request": "500m",
          "limit": "2000m"
        },
        "memory": {
          "request": "1Gi",
          "limit": "4Gi"
        }
      }
    },
    "channels": {
      "email": {
        "provider": "sendgrid",
        "config": {
          "api_key_secret": "sendgrid-api-key",
          "from_address": "noreply@example.com",
          "from_name": "Example Inc"
        },
        "templates": {
          "welcome": "d-abc123",
          "password_reset": "d-def456",
          "invoice": "d-ghi789"
        },
        "rate_limit": {
          "per_user": 10,
          "per_minute": 1000
        }
      },
      "sms": {
        "provider": "twilio",
        "config": {
          "account_sid_secret": "twilio-sid",
          "auth_token_secret": "twilio-token",
          "from_number": "+15551234567"
        },
        "fallback": {
          "enabled": true,
          "provider": "aws-sns"
        }
      },
      "push": {
        "provider": "firebase",
        "config": {
          "credentials_secret": "firebase-credentials",
          "project_id": "example-app"
        },
        "platforms": {
          "ios": {
            "badge_count": true,
            "sound": "default"
          },
          "android": {
            "priority": "high",
            "ttl": "3600s"
          }
        }
      }
    },
    "rules": {
      "throttling": {
        "enabled": true,
        "window": "1h",
        "max_per_user": {
          "email": 50,
          "sms": 10,
          "push": 100
        }
      },
      "preferences": {
        "respect_quiet_hours": true,
        "quiet_hours": {
          "start": "22:00",
          "end": "08:00",
          "timezone": "user"
        }
      }
    }
  },
  {
    "service_name": "search-service",
    "version": "4.2.1",
    "environment": "production",
    "deployment": {
      "strategy": "rolling",
      "replicas": 8,
      "resources": {
        "cpu": {
          "request": "3000m",
          "limit": "12000m"
        },
        "memory": {
          "request": "8Gi",
          "limit": "32Gi"
        }
      }
    },
    "elasticsearch": {
      "cluster": {
        "nodes": ["es-1:9200", "es-2:9200", "es-3:9200"],
        "username_secret": "es-username",
        "password_secret": "es-password"
      },
      "indices": {
        "products": {
          "shards": 5,
          "replicas": 2,
          "mappings": {
            "properties": {
              "title": {
                "type": "text",
                "analyzer": "standard"
              },
              "price": {
                "type": "float"
              },
              "category": {
                "type": "keyword"
              }
            }
          },
          "settings": {
            "refresh_interval": "1s",
            "max_result_window": 10000
          }
        }
      },
      "search": {
        "default_size": 20,
        "max_size": 100,
        "timeout": "5s",
        "highlight": {
          "enabled": true,
          "pre_tags": ["<mark>"],
          "post_tags": ["</mark>"]
        }
      }
    },
    "ml": {
      "recommendations": {
        "enabled": true,
        "model": {
          "type": "collaborative-filtering",
          "version": "v2.1",
          "endpoint": "https://ml.internal/recommend"
        },
        "cache": {
          "ttl": "30m",
          "max_items": 1000
        }
      }
    }
  },
  {
    "service_name": "analytics-pipeline",
    "version": "1.5.2",
    "environment": "production",
    "deployment": {
      "strategy": "rolling",
      "replicas": 10,
      "resources": {
        "cpu": {
          "request": "4000m",
          "limit": "16000m"
        },
        "memory": {
          "request": "16Gi",
          "limit": "64Gi"
        }
      }
    },
    "data_sources": {
      "clickstream": {
        "kafka": {
          "topics": ["page_views", "clicks", "conversions"],
          "consumer_group": "analytics-processor",
          "batch": {
            "size": 1000,
            "timeout": "10s"
          }
        }
      },
      "databases": {
        "postgres": {
          "connections": ["db1", "db2", "db3"],
          "sync_interval": "5m"
        }
      }
    },
    "processing": {
      "spark": {
        "master": "spark://master:7077",
        "executor": {
          "instances": 20,
          "cores": 4,
          "memory": "8g"
        },
        "driver": {
          "cores": 2,
          "memory": "4g"
        },
        "conf": {
          "spark.sql.shuffle.partitions": "200",
          "spark.default.parallelism": "100"
        }
      },
      "streaming": {
        "window": {
          "size": "1m",
          "slide": "10s"
        },
        "watermark": "5m"
      }
    },
    "storage": {
      "s3": {
        "bucket": "analytics-data",
        "prefix": "events/",
        "partition_by": ["year", "month", "day"],
        "format": "parquet",
        "compression": "snappy"
      }
    }
  },
  {
    "service_name": "auth-service",
    "version": "2.3.4",
    "environment": "production",
    "deployment": {
      "strategy": "blue-green",
      "replicas": 4,
      "resources": {
        "cpu": {
          "request": "1000m",
          "limit": "4000m"
        },
        "memory": {
          "request": "2Gi",
          "limit": "8Gi"
        }
      }
    },
    "authentication": {
      "methods": {
        "password": {
          "enabled": true,
          "min_length": 12,
